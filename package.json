{
  "module": "commonjs",
  "name": "tree-sitter-gloss",
  "version": "0.1.0",
  "description": "Gloss grammar for tree-sitter",
  "repository": "https://github.com/sjwalker189/tree-sitter-gloss",
  "license": "MIT",
  "author": {
    "name": "Sam Walker",
    "email": "sjwalker189@gmail.com"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "gloss"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.26.5"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "gen": "tree-sitter generate",
    "build": "tree-sitter generate && tree-sitter build --wasm",
    "prestart": "npm run build",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "hl": "tree-sitter highlight"
  }
}
