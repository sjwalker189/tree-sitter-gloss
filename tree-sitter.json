{
  "grammars": [
    {
      "name": "gloss",
      "camelcase": "Gloss",
      "scope": "source.gloss",
      "path": ".",
      "file-types": [
        "gloss"
      ],
      "injection-regex": "^gloss$",
      "highlights": [
        "queries/highlights.scm"
      ],
      "injections": [
        "queries/injections.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ]
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Gloss grammar for tree-sitter",
    "authors": [
      {
        "name": "Sam Walker",
        "email": "sjwalker189@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/sjwalker189/tree-sitter-gloss"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
